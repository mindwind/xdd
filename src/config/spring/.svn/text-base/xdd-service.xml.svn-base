<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd" >
<beans>

	<bean id="welcomeAction"
		class="org.craft.xdd.view.action.WelcomeAction" singleton="false">
		<property name="userDao" ref="userDao" />
	</bean>

	<bean id="indexAction" class="org.craft.xdd.view.action.IndexAction"
		singleton="false">
		<property name="infoGroupDao" ref="infoGroupDao" />
		<property name="infoDao" ref="infoDao" />
		<property name="orgDao" ref="orgDao" />
	</bean>

</beans>